<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css">
<link rel="stylesheet" href="app.css">

<ul>
    <% movies.forEach( movie => { %>
        <li>
            <% if( movie.Poster !== 'N/A') { %>
            <img src=<%= movie.Poster %>>
            <% } %>
            <div class="title">
                <%= movie.Title %>
            </div>

        </li>
    <% }); %>
</ul>